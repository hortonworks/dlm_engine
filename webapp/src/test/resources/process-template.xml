<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<process name="##name##">
    <cluster name="##cluster##" />
	
	<concurrency>2</concurrency>
	<execution>LIFO</execution>
    <frequency>minutes</frequency>
    <periodicity>30</periodicity>
	
	<validity timezone="UTC" end="2010-01-01T01:30Z" start="2010-01-01T01:00Z" />
	<inputs>
		<input end-instance="today(0,40)" start-instance="today(0,20)"
			feed="##f1##" name="inputData"/>
	</inputs>
	<outputs>
		<output instance="now(0,0)" feed="##f2##"
			name="outputData" />
	</outputs>
	<properties>
	</properties>
	<workflow engine="oozie" path="/examples/apps/aggregator" />
	<retry policy="backoff" delay="10" delayUnit="minutes" attempts="3" />

    <late-process policy="exp-backoff" delay="1" delayUnit="hours">
        <late-input feed="impression" workflow-path="hdfs://impression/late/workflow" />
        <late-input feed="clicks" workflow-path="hdfs://clicks/late/workflow" />
    </late-process>
</process>